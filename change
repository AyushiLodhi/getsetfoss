$.getJSON("./Data/participants.json", (data) => {
      var str = data.participants.map((v, i) => {
        return `<div class="card" id="participants" style="width:200px">
        <img id="participants-img" src="${v.profilePicture}" alt="Card image"
          style="width:100%" />

        <div class="link-layer">
          <div class="table-layer">
            <div class="cell-layer">
              <span>
                <a href="#" id="participants-links"><i class="lni-facebook-filled"></i></a>
                <a href="#" id="participants-links"><i class="lni-github-original"></i></a>
              </span>
            </div>
          </div>
        </div>

        <b id="participants-name">nawab-shah</b>
      </div>`
      })
      $("#participant-cards-section").html(str);
    })



    /* ==========================================================================
   Participants Section
   ========================================================================== */

  .card#participants{
