    height: 100%;
    left: 0;
    opacity: 0;
    position: absolute;
    top: 0;
    width: 100%;
    padding: 1vw;
  }

  .link-layer .table-layer{
    display: table;
    height: 100%;
    left: 0;
    position: relative;
    top: 0;
    width: 100%;
    z-index: 2;
  }

  .link-layer .table-layer .cell-layer{
    display: table-cell;
    vertical-align: middle;
    text-align: center;
  }

  .link-layer:hover{
    opacity: 1;
  }

  .card#participants:hover img {
    -webkit-transform: scale(1.1);
    -ms-transform: scale(1.1);
    transform: scale(1.1);
    
    transition: all 0.6s ease;
    -moz-transition: all 0.6s ease;
    -webkit-transition: all 0.6s ease;
  }

  .participants-name{
    font-size: 1.2vw;
